%-----------EXPERIENCE-----------------
\section{Experience}
  \resumeSubHeadingListStart
{% set prev_company = namespace(value='') %}
{% for exp in cv.experiences %}
{% if prev_company.value == exp.company %}
    \resumeSubSubheading
      { {{- exp.position | escape_tex -}} }{ {{- exp.period | format_period -}} }
{% else %}
    \resumeSubheading
      { {{- exp.company | escape_tex -}} }{ {{- exp.location | escape_tex -}} }
      { {{- exp.position | escape_tex -}} }{ {{- exp.period | format_period -}} }
{% endif %}
      \resumeItemListStart
{% for group_name, items in exp.achievement_groups.items() %}
{% if items | length > 1 %}
        \resumeItemTitleAndStart{ {{- group_name | escape_tex -}} }
{% for item in items %}
          \item{ {{- item | escape_tex -}} \vspace{-2pt}}
{% endfor %}
        \end{itemize}
{% else %}
        \resumeItem{ {{- group_name | escape_tex -}} }
          { {{- items[0] | escape_tex -}} \vspace{-2pt}}
{% endif %}
{% endfor %}
{% set ungrouped = [] %}
{% for ach in exp.achievements %}
{% set found = namespace(value=false) %}
{% for group_name, items in exp.achievement_groups.items() %}
{% if ach in items %}
{% set found.value = true %}
{% endif %}
{% endfor %}
{% if not found.value %}
{% set _ = ungrouped.append(ach) %}
{% endif %}
{% endfor %}
{% for item in ungrouped %}
        \item\small{ {{- item | escape_tex -}} \vspace{-2pt}}
{% endfor %}
      \resumeItemListEnd
{% set prev_company.value = exp.company %}
{% endfor %}

  \resumeSubHeadingListEnd


